{
  "name": "Switch Proxy",
  "description": "Quickly turn on/off SOCKS5 proxy",
  "version": "1.0",
  "manifest_version": 2,
  "permissions": ["storage","proxy"],
  "options_page": "options.html",
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_icon" : "icon.png",
    "default_title": "Switch Proxy"
  }
}
